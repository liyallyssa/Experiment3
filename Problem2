#include <iostream>
#include <conio.h>
#include <string>

using namespace std;

int main()
{
  int provinceA[7] = { 32, 33, 32, 34 , 35, 36, 38 };
  int provinceB[7] = { 23, 24, 26, 22 , 29, 27, 23 };
  int provinceC[7] = { 30, 29, 26, 28 , 29, 27, 29 };
  int dayCounterA = 1;
  int dayCounterB = 1;
  int dayCounterC = 1;

  for (int i = 0; i < 7; i++, dayCounterA++)
  {
    cout << "Province A, Day " << dayCounterA << "= " << provinceA[i] << endl;
  }
  for (int i = 0; i < 7; i++, dayCounterB++)
  {
    cout << "Province B, Day " << dayCounterB << "= " << provinceB[i] << endl;
  }
  for (int i = 0; i < 7; i++, dayCounterC++)
  {
    cout << "Province C, Day " << dayCounterC << "= " << provinceC[i] << endl;
  }
  
  _getch();
  return 0;
}
